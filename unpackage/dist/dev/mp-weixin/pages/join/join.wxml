<view class="content"><view class="upload-msg"><view class="item"><view class="item-t">门诊名称</view><view class="item-b"><input type="text" placeholder="请输入门诊名称" data-event-opts="{{[['input',[['__set_model',['','outpatientName','$event',[]]]]]]}}" value="{{outpatientName}}" bindinput="__e"/></view></view><view class="item"><view class="item-t">医疗机构执业许可证登记号</view><view class="item-b"><input type="text" placeholder="请输入医疗机构执业许可证登记号" data-event-opts="{{[['input',[['__set_model',['','registrationNum','$event',[]]]]]]}}" value="{{registrationNum}}" bindinput="__e"/></view></view><view class="item"><view class="item-t">法人姓名</view><view class="item-b"><input type="text" placeholder="请输入法人姓名" data-event-opts="{{[['input',[['__set_model',['','legalName','$event',[]]]]]]}}" value="{{legalName}}" bindinput="__e"/></view></view><view class="item"><view class="item-t">联系人姓名</view><view class="item-b"><input type="text" placeholder="请输入联系人姓名" data-event-opts="{{[['input',[['__set_model',['','contactName','$event',[]]]]]]}}" value="{{contactName}}" bindinput="__e"/></view></view><view class="item"><view class="item-t">地址</view><view class="item-b"><input type="text" placeholder="请输入地址" data-event-opts="{{[['input',[['__set_model',['','address','$event',[]]]]]]}}" value="{{address}}" bindinput="__e"/></view></view><view class="item"><view class="item-t">联系人电话</view><view class="item-b"><input type="number" placeholder="请输入联系人电话" data-event-opts="{{[['input',[['__set_model',['','telephone','$event',[]]]]]]}}" value="{{telephone}}" bindinput="__e"/></view></view></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btn-save" bindtap="__e">提交加盟信息</view></view>